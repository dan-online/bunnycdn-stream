{
  "name": "bunnycdn-stream",
  "version": "1.0.2",
  "description": "",
  "author": "DanCodes <dan@dancodes.online> (https://dancodes.online/)",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist node_modules/.cache",
    "lint": "eslint src tests --ext ts --fix",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "test": "jest",
    "update": "yarn upgrade-interactive",
    "build": "rollup -c rollup.bundle.ts",
    "docs": "cd src/; typedoc --plugin typedoc-plugin-markdown --out ../docs ./index.ts",
    "watch": "tsc -b src -w",
    "release": "npm publish",
    "prepublishOnly": "rollup-type-bundler",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@favware/npm-deprecate": "^1.0.5",
    "@favware/rollup-type-bundler": "^1.0.11",
    "@sapphire/eslint-config": "~4.3.8",
    "@sapphire/ts-config": "^3.3.4",
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.23",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-circus": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "rollup-plugin-version-injector": "^1.3.3",
    "standard-version": "^9.3.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.14",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.3"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=6"
  },
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.ts": "eslint --fix --ext ts"
  },
  "packageManager": "yarn@3.2.3",
  "dependencies": {
    "axios": "^0.27.2"
  }
}
